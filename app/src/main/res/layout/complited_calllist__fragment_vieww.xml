<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/square_border"
        android:layout_marginBottom="10sp">
        <TextView
            android:id="@+id/imgID"
            android:layout_marginTop="10sp"
            android:layout_width="30sp"
            android:layout_height="30sp"
            android:gravity="center"
            android:text="1"
            android:textColor="@color/white"
            android:background="@drawable/ovalview"
            android:layout_marginLeft="5sp"/>
        <TextView
            android:layout_toRightOf="@+id/imgID"
            android:layout_marginTop="5sp"
            android:id="@+id/nameId"
            android:textSize="14sp"
            android:layout_marginLeft="10sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Microwave Oven Repair"
            android:textStyle="bold"
            android:textColor="@color/black"/>
        <TextView
            android:id="@+id/amountId"
            android:text="25102"
            android:drawableLeft="@drawable/rupeess"
            android:textColor="@color/red"
            android:textSize="14sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignBaseline="@+id/nameId"
            android:layout_marginRight="20sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/bId"
            android:text="25102"
            android:visibility="gone"
            android:textColor="@color/red"
            android:textSize="14sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignBaseline="@+id/calldatetimeId"
            android:layout_marginRight="20sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/calldatetimeId"
            android:textSize="14sp"
            android:layout_marginTop="5sp"
            android:layout_below="@+id/nameId"
            android:layout_toRightOf="@+id/imgID"
            android:layout_marginLeft="10sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1st November"/>

        <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_marginTop="10sp"
            android:layout_toRightOf="@+id/imgID"
            android:id="@+id/tab"
            android:layout_below="@+id/calldatetimeId">
            <TableRow
                android:padding="1dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:text="Job Assign Date" />
                <TextView
                    android:id="@+id/jobasdateId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:text="05/11/2019" />
            </TableRow>
            <TableRow
                android:padding="1dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:text="Job Assign Time" />
                <TextView
                    android:id="@+id/timeasId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:text="02:54 pm" />
            </TableRow>
            <TableRow
                android:padding="1dp"
                android:layout_marginBottom="15sp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:text="Service Type" />
                <TextView
                    android:id="@+id/serviceId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:text="Breakdown Service" />
            </TableRow>
        </TableLayout>
